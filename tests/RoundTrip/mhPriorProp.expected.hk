(fn x5 pair(real, real): 
  (match x5: 
    (r3, r1): 
     weight(1/2,
            x159 <~ normal(+0/1, 1/1)
            return ((x159, r1),
                    (recip(exp((r1 * r3)))
                      * 
                     exp((x159 * r1))
                      * 
                     exp((r3 ^ 2))
                      / 
                     exp((x159 ^ 2))))) <|> 
     weight(1/2,
            x197 <~ normal(+0/1, sqrt(2/1))
            return ((r3, x197),
                    (exp(((r1 ^ 2) * (+1/2)))
                      / 
                     exp((r1 * r3))
                      * 
                     exp((x197 * r3))
                      * 
                     exp(((x197 ^ 2) * (-1/2))))))))
